---
import { getProjecData } from "@/services/projects-project";
import Layout from "@/layouts/Layout.astro";
import HeroProjects from "@/components/ProjectsComponents/HeroProjects.astro";
import IntroProjects from "@/components/ProjectsComponents/IntroProjects.astro";

const projectData = await getProjecData();
const { hero, projectHistory } = projectData;
---

<Layout>
    <HeroProjects hero={hero} />
    <IntroProjects Projects={projectHistory} />
</Layout>
